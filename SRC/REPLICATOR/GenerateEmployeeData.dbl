function randomPhone, string
    literal
        part1Count, i4, 999
        part2Count, i4, 9999
    endliteral
    static record
        now, a20
    endrecord
    stack record
        randomNumber, i4
        part1, i4
        part2, i4
    endrecord
proc
    if (!now) now=%datetime
    xcall randm(randomNumber,^d(now(1:8)),^d(now(9:8)))
    part1 = %rnd(randomNumber/(32767.0/(part1Count-1)))+1
    xcall randm(randomNumber,^d(now(1:8)),^d(now(9:8)))
    part2 = %rnd(randomNumber/(32767.0/(part2Count-1)))+1
    freturn "(555) " + %string(part1,"XXX") + "-" + %string(part2,"XXXX")
endfunction

function randomEmail, string
    required in aFirstName, string
    required in aLastName, string
    literal
        itemCount, i4, 3
        domain, [itemcount]a30, "@gmail.com","@yahoo.com","@outlook.com"
    endliteral
    static record
        now, a20
    endrecord
    stack record
        randomNumber, i4
        index, i4
        firstName, string
        lastName, string
    endrecord
proc
    if (!now) now=%datetime
    xcall randm(randomNumber,^d(now(1:8)),^d(now(9:8)))
    index = %rnd(randomNumber/(32767.0/(itemCount-1)))+1
    firstName = %atrim(aFirstName)
    lastName = %atrim(aLastName)
    freturn firstName.ToLower() + "." + lastName.ToLower() + %atrim(domain[index])
endfunction


