function randomDateOfBirth, d8
    static record
        now, a20
        julianNow, i4
        julianAge18, i4
        julianAge65, i4
        daysRange, i4
    endrecord
    stack record
        randomNumber, i4
        randomDaysOver18, i4
        randomDate, d8
    endrecord
proc
    if (!now)
    begin
        now=%datetime
        julianNow = %jperiod(^d(now(1:8)))
        julianAge18 = julianNow-(18*365)
        julianAge65 = julianNow-(65*365)
        daysRange = julianAge18 - julianAge65
    end
    xcall randm(randomNumber,^d(now(1:8)),^d(now(9:8)))
    randomDaysOver18 = %rnd(randomNumber/(32767.0/(daysRange-1)))+1
    randomDate = %ndate(julianAge18-randomDaysOver18)
    freturn randomDate
endfunction

function randomHireDate, d8
    required in aDateOfBirth, d8
    static record
        now, a20
    endrecord
    stack record
        julianDateOfBirth, i4
        julianBirthday18, i4
        julianBirthday60, i4
        daysRange, i4
        randomNumber, i4
        randomDaysOver18, i4
        randomDate, d8
    endrecord
proc
    ;Random date between the employees 18th and 60th birthday
    if (!now) now = %datetime

    julianDateOfBirth = %jperiod(aDateOfBirth)
    julianBirthday18 = julianDateOfBirth+(18*365)
    julianBirthday60 = julianDateOfBirth+(60*365)
    daysRange = julianBirthday60 - julianBirthday18

    xcall randm(randomNumber,^d(now(1:8)),^d(now(9:8)))
    randomDaysOver18 = %rnd(randomNumber/(32767.0/(daysRange-1)))+1
    randomDate = %ndate(julianBirthday18+randomDaysOver18)

    freturn randomDate

endfunction

